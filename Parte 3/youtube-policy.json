{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject",
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::videos",
				"arn:aws:s3:::videos/*",
				"arn:aws:s3:::shorts",
				"arn:aws:s3:::shorts/*",
				"arn:aws:s3:::lives",
				"arn:aws:s3:::lives/*",
				"arn:aws:s3:::images",
				"arn:aws:s3:::images/*",
				"arn:aws:s3:::temp",
				"arn:aws:s3:::temp/*"
			]
		}
	]
}